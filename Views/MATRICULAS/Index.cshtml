@model IEnumerable<MECTRONICS.Models.MATRICULAS>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            CLASE
        </th>
        <th>
            ALUMNO
        </th>
        <th>
            NOTA
        </th>
        <th>
            FECHA DE LA NOTA
        </th>


        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
