        @Html.DisplayFor(modelItem => item.CLASES.ID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ALUMNOS.DOCUMENTO)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.NOTA)
    </td>
